		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#94f884"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#24fc03"
			}
		},
